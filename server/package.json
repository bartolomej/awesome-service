{
  "name": "awesomesearch-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/worker.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "jest",
    "test:load:staging": "yarn run start:web:dev && artillery run -e staging src/test/load-test.yml ",
    "test:load:production": "yarn run start:web:dev && artillery run -e production src/test/load-test.yml ",
    "build": "tsc && yarn run copy-assets",
    "copy-assets": "cp -r src/web/public/ build/web/public",
    "start": "NODE_ENV=production ENV_PATH=.env.production nf start",
    "start:dev": "NODE_ENV=development ENV_PATH=.env.development nf start",
    "start:web:dev": "NODE_ENV=development ENV_PATH=.env.development nodemon src/web/index.ts",
    "start:web:prod": "NODE_ENV=production ENV_PATH=.env.production nodemon src/web/index.ts",
    "docker-compose:up:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker-compose:up:dev": "docker-compose up --build",
    "coverage": "jest --coverage",
    "coverage:web": "npm run coverage && open coverage/lcov-report/index.html",
    "repo-graph-ddot": "depcruise --exclude \"^node_modules\" --output-type ddot src | dot -T svg > graph.ddot.svg",
    "repo-graph-dot": "depcruise --exclude \"^node_modules\" --output-type dot src | dot -T svg > graph.dot.svg",
    "loc": "find ./src -name '*.js' | xargs wc -l | sort -nr",
    "db:seed": "N_LISTS=20 N_LINKS_PER_LIST=5 node src/scripts/seed-db.ts",
    "db:reset": "node src/scripts/reset-db.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartolomej/awesome-server.git"
  },
  "author": "Bartolomej Kozorog",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bartolomej/awesomesearch-api/issues"
  },
  "homepage": "https://github.com/bartolomej/awesomesearch-api#readme",
  "dependencies": {
    "@jest/globals": "26.0.1",
    "@types/bull": "^3.14.2",
    "@types/cheerio": "0.22.21",
    "@types/jest": "26.0.10",
    "artillery": "1.6.1",
    "bull": "3.14.0",
    "bull-board": "0.7.0",
    "chalk": "3.0.0",
    "cheerio": "1.0.0-rc.3",
    "cloudinary": "1.21.0",
    "colors": "latest",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "envalid": "6.0.1",
    "express": "4.17.1",
    "express-useragent": "1.0.13",
    "express-validator": "6.6.0",
    "faker": "4.1.0",
    "fetch-mock": "9.2.1",
    "flexsearch": "0.6.32",
    "foreman": "3.0.1",
    "ioredis": "4.16.3",
    "jest": "25.1.0",
    "meow": "6.0.1",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "node-fetch": "2.6.0",
    "normalize-url": "5.0.0",
    "parse-github-url": "1.0.2",
    "puppeteer": "3.0.4",
    "reflect-metadata": "0.1.13",
    "remark-parse": "7.0.2",
    "rimraf": "3.0.2",
    "throng": "4.0.0",
    "ts-jest": "26.3.0",
    "typeorm": "0.2.25",
    "typescript": "4.0.2",
    "ua-parser-js": "0.7.24",
    "unified": "8.4.2",
    "unist-util-select": "3.0.1",
    "uuid": "8.3.0",
    "winston": "3.2.1"
  }
}
